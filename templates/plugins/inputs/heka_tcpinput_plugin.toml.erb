[<%= @name %>]
type = "TcpInput"
<%- if @address -%>
address = "<%= @address -%>"
<%- end -%>
<%- if @decoder -%>
decoder = "<%= @decoder -%>"
<%- end -%>
<%- if @parser_type -%>
parser_type = "<%= @parser_type -%>"
<%- end -%>
<%- if @use_tls -%>
use_tls = <%= @use_tls -%>
<%- end -%>
<%- if @net -%>
net = "<%= @net -%>"
<%- end -%>
<%- if @keep_alive -%>
keep_alive = <%= @keep_alive -%>
<%- end -%>
<%- if @keep_alive_period -%>
keep_alive_period = "<%= @keep_alive_period -%>"
<%- end -%>
<%- if @splitter -%>
splitter = "<%= @splitter -%>"
<%- end -%>
